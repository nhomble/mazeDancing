#!/usr/bin/env python2

import time
import sys

# ROS specifoc
import rospy

# my modules
from Move_Manager  import *

def checks(move):
	print(move._checks)

# ROS loop
def main(move):
	while not rospy.is_shutdown():
		checks(move)
		rospy.sleep(1)
	return 

if __name__ == "__main__":
	try:
		rospy.init_node('wall_follower')
		move = Move_Manager()
		sys.exit(main(move))
	except rospy.ROSInterruptException:
		pass
